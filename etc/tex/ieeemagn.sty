% default captions are footnotesize
\long\def\@makecaption#1#2{
 \vskip 10pt
 \footnotesize
 \setbox\@tempboxa\hbox{#1 #2}
 \ifdim \wd\@tempboxa >\hsize #1 #2\par \else \hbox
to\hsize{\hfil\box\@tempboxa\hfil}
 \fi}

